          "name": "stdout",
          "output_type": "stream",
          "text": [
            "\n",
            "Enter Employee Details:\n"
          ]
        }
      ],
      "source": [
        "# Enhancing Employee Engagement Using Hybrid Random Forest and XGBoost Approach\n",
        "import pandas as pd\n",
        "import numpy as np\n",
        "from sklearn.model_selection import train_test_split\n",
        "from sklearn.ensemble import RandomForestClassifier, VotingClassifier\n",
        "from sklearn.metrics import classification_report, confusion_matrix, roc_curve, auc, accuracy_score\n",
        "from sklearn.preprocessing import LabelEncoder, StandardScaler\n",
        "from imblearn.over_sampling import SMOTE\n",
        "from xgboost import XGBClassifier\n",
        "import matplotlib.pyplot as plt\n",
        "import seaborn as sns\n",
        "import warnings\n",
        "warnings.filterwarnings(\"ignore\")\n",
        "\n",
        "# Input\n",
        "def safe_input(prompt, cast_type, min_val=None, max_val=None):\n",
        "    while True:\n",
        "        try:\n",
        "            val = cast_type(input(prompt))\n",
        "            if (min_val is not None and val < min_val) or (max_val is not None and val > max_val):\n",
        "                raise ValueError\n",
        "            return val\n",
        "        except:\n",
        "            print(f\"Please enter a valid {cast_type.__name__} between {min_val} and {max_val}\")\n",
        "\n",
        "def safe_choice(prompt, choices):\n",
        "    while True:\n",
        "        val = input(prompt).strip()\n",
        "        if val in choices:\n",
        "            return val\n",
        "        print(f\"Invalid input. Choose from: {', '.join(choices)}\")\n",
        "\n",
        "def get_user_input():\n",
        "    print(\"\\nEnter Employee Details:\")\n",
        "    age = safe_input(\"Age (22-60): \", int, 22, 60)\n",
        "    dept = safe_choice(\"Department (Logistics, Operations, HR, Finance): \", [\"Logistics\", \"Operations\", \"HR\", \"Finance\"])\n",
        "    role = safe_choice(\"Job Role (Manager, Executive, Clerk, Driver): \", [\"Manager\", \"Executive\", \"Clerk\", \"Driver\"])\n",
        "    work_hours = safe_input(\"Work Hours per Week (30-60): \", int, 30, 60)\n",
        "    remote = safe_input(\"Remote Work Available (1-Yes, 0-No): \", int, 0, 1)\n",
        "    training = safe_input(\"Training Opportunities (1-Yes, 0-No): \", int, 0, 1)\n",
        "    progression = safe_input(\"Career Progression Opportunities (1-Yes, 0-No): \", int, 0, 1)\n",
        "    feedback = safe_input(\"Feedback Frequency (1-10): \", int, 1, 10)\n",
        "    support = safe_input(\"Leadership Support (1-5): \", int, 1, 5)\n",
        "    wlb = safe_input(\"Work-Life Balance Score (1-10): \", int, 1, 10)\n",
        "    salary = safe_input(\"Salary (20000-80000): \", int, 20000, 80000)\n",
        "    delivery_rate = safe_input(\"Delivery Success Rate (70-100): \", float, 70, 100)\n",
        "    warehouse_time = safe_input(\"Warehouse Hours per Day (5-15): \", int, 5, 15)\n",
        "\n",
        "    return pd.DataFrame([{\n",
        "        \"Age\": age,\n",
        "        \"Department\": dept,\n",
        "        \"JobRole\": role,\n",
        "        \"WorkHoursPerWeek\": work_hours,\n",
        "        \"RemoteWorkAvailable\": remote,\n",
        "        \"TrainingOpportunities\": training,\n",
        "        \"CareerProgression\": progression,\n",
        "        \"FeedbackFrequency\": feedback,\n",
        "        \"LeadershipSupport\": support,\n",
        "        \"WorkLifeBalance\": wlb,\n",
        "        \"Salary\": salary,\n",
        "        \"DeliverySuccessRate\": delivery_rate,\n",
        "        \"WarehouseHours\": warehouse_time\n",
        "    }])\n",
        "\n",
        "# Data\n",
        "np.random.seed(42)\n",
        "n_samples = 3000\n",
        "\n",
        "data = {\n",
        "    \"Age\": np.random.randint(22, 60, size=n_samples),\n",
        "    \"Department\": np.random.choice([\"Logistics\", \"Operations\", \"HR\", \"Finance\"], size=n_samples),\n",
        "    \"JobRole\": np.random.choice([\"Manager\", \"Executive\", \"Clerk\", \"Driver\"], size=n_samples),\n",
        "    \"WorkHoursPerWeek\": np.random.randint(30, 60, size=n_samples),\n",
        "    \"RemoteWorkAvailable\": np.random.choice([0, 1], size=n_samples),\n",
        "    \"TrainingOpportunities\": np.random.choice([0, 1], size=n_samples),\n",
        "    \"CareerProgression\": np.random.choice([0, 1], size=n_samples),\n",
        "    \"FeedbackFrequency\": np.random.randint(1, 11, size=n_samples),\n",
        "    \"LeadershipSupport\": np.random.randint(1, 6, size=n_samples),\n",
        "    \"WorkLifeBalance\": np.random.randint(1, 11, size=n_samples),\n",
        "    \"Salary\": np.random.randint(20000, 80000, size=n_samples),\n",
        "    \"DeliverySuccessRate\": np.random.uniform(70, 100, size=n_samples),\n",
        "    \"WarehouseHours\": np.random.randint(5, 15, size=n_samples),\n",
        "}\n",
        "\n",
        "engagement_score = (\n",
        "    0.25 * data[\"LeadershipSupport\"] +\n",
        "    0.25 * data[\"WorkLifeBalance\"] +\n",
        "    0.15 * data[\"TrainingOpportunities\"] +\n",
        "    0.15 * data[\"CareerProgression\"] +\n",
        "    0.1 * data[\"RemoteWorkAvailable\"] +\n",
        "    0.1 * (data[\"DeliverySuccessRate\"] / 100)\n",
        ")\n",
        "\n",
        "engagement_score = (engagement_score - np.min(engagement_score)) / (np.max(engagement_score) - np.min(engagement_score))\n",
        "data[\"Engagement\"] = (engagement_score > 0.5).astype(int)\n",
        "df = pd.DataFrame(data)\n",
        "\n",
        "# Encoding & Scaling\n",
        "label_encoders = {}\n",
        "for col in [\"Department\", \"JobRole\"]:\n",
        "    le = LabelEncoder()\n",
        "    df[col] = le.fit_transform(df[col])\n",
        "    label_encoders[col] = le\n",
        "\n",
        "X = df.drop(\"Engagement\", axis=1)\n",
        "y = df[\"Engagement\"]\n",
        "scaler = StandardScaler()\n",
        "X_scaled = scaler.fit_transform(X)\n",
        "\n",
        "# Balancing\n",
        "sm = SMOTE(random_state=42)\n",
        "X_resampled, y_resampled = sm.fit_resample(X_scaled, y)\n",
        "\n",
        "# Model Training\n",
        "X_train, X_test, y_train, y_test = train_test_split(X_resampled, y_resampled, test_size=0.2, random_state=42)\n",
        "\n",
        "rf = RandomForestClassifier(\n",
        "    n_estimators=500,\n",
        "    max_depth=30,\n",
        "    min_samples_split=4,\n",
        "    min_samples_leaf=2,\n",
        "    random_state=42\n",
        ")\n",
        "\n",
        "xgb = XGBClassifier(\n",
        "    use_label_encoder=False,\n",
        "    eval_metric='logloss',\n",
        "    n_estimators=50,\n",
        "    max_depth=2,\n",
        "    learning_rate=0.4,\n",
        "    subsample=0.5,\n",
        "    colsample_bytree=0.5,\n",
        "    random_state=42\n",
        ")\n",
        "\n",
        "hybrid_model = VotingClassifier(\n",
        "    estimators=[('rf', rf), ('xgb', xgb)],\n",
        "    voting='soft',\n",
        "    weights=[3, 1]\n",
        ")\n",
        "\n",
        "# Fit models\n",
        "rf.fit(X_train, y_train)\n",
        "xgb.fit(X_train, y_train)\n",
        "hybrid_model.fit(X_train, y_train)\n",
        "\n",
        "# Evaluation\n",
        "rf_pred = rf.predict(X_test)\n",
        "xgb_pred = xgb.predict(X_test)\n",
        "hyb_pred = hybrid_model.predict(X_test)\n",
        "\n",
        "print(\"\\n=== Random Forest Report ===\")\n",
        "print(classification_report(y_test, rf_pred))\n",
        "\n",
        "print(\"\\n=== XGBoost Report ===\")\n",
        "print(classification_report(y_test, xgb_pred))\n",
        "\n",
        "print(\"\\n=== Hybrid Model Report ===\")\n",
        "print(classification_report(y_test, hyb_pred))\n",
        "\n",
        "# Accuracy Comparison\n",
        "rf_acc = accuracy_score(y_test, rf_pred)\n",
        "xgb_acc = accuracy_score(y_test, xgb_pred)\n",
        "hyb_acc = accuracy_score(y_test, hyb_pred)\n",
        "\n",
        "print(f\"\\nAccuracy Scores:\\n  Random Forest: {rf_acc:.4f}\\n  XGBoost: {xgb_acc:.4f}\\n  Hybrid: {hyb_acc:.4f}\")\n",
        "\n",
        "# Visualization\n",
        "cm = confusion_matrix(y_test, hyb_pred)\n",
        "plt.figure(figsize=(6, 4))\n",
        "sns.heatmap(cm, annot=True, fmt='d', cmap='Blues', xticklabels=[\"Low\", \"High\"], yticklabels=[\"Low\", \"High\"])\n",
        "plt.xlabel(\"Predicted\")\n",
        "plt.ylabel(\"Actual\")\n",
        "plt.title(\"Confusion Matrix - Hybrid\")\n",
        "plt.show()\n",
        "\n",
        "# ROC Curve\n",
        "fpr_rf, tpr_rf, _ = roc_curve(y_test, rf.predict_proba(X_test)[:, 1])\n",
        "fpr_xgb, tpr_xgb, _ = roc_curve(y_test, xgb.predict_proba(X_test)[:, 1])\n",
        "fpr_hyb, tpr_hyb, _ = roc_curve(y_test, hybrid_model.predict_proba(X_test)[:, 1])\n",
        "\n",
        "plt.figure(figsize=(8, 6))\n",
        "plt.plot(fpr_rf, tpr_rf, label=f'Random Forest (AUC = {auc(fpr_rf, tpr_rf):.2f})')\n",
        "plt.plot(fpr_xgb, tpr_xgb, label=f'XGBoost (AUC = {auc(fpr_xgb, tpr_xgb):.2f})')\n",
        "plt.plot(fpr_hyb, tpr_hyb, label=f'Hybrid (AUC = {auc(fpr_hyb, tpr_hyb):.2f})')\n",
        "plt.plot([0, 1], [0, 1], 'k--', label='Random Guess')\n",
        "plt.title('ROC Curve Comparison')\n",
        "plt.xlabel('False Positive Rate')\n",
        "plt.ylabel('True Positive Rate')\n",
        "plt.legend(loc='lower right')\n",
        "plt.grid(True)\n",
        "plt.tight_layout()\n",
        "plt.show()\n",
        "\n",
        "# Accuracy Bar Plot\n",
        "plt.figure(figsize=(8, 5))\n",
        "sns.barplot(x=['Random Forest', 'XGBoost', 'Hybrid'], y=[rf_acc, xgb_acc, hyb_acc])\n",
        "plt.title('Model Accuracy Comparison')\n",
        "plt.ylim(0.90, 1.00)\n",
        "plt.ylabel(\"Accuracy\")\n",
        "plt.show()\n",
        "\n",
        "# Feature Importances\n",
        "importances = rf.feature_importances_\n",
        "importance_df = pd.DataFrame({\"Feature\": X.columns, \"Importance\": importances}).sort_values(by=\"Importance\", ascending=False)\n",
        "\n",
        "plt.figure(figsize=(10, 6))\n",
        "sns.barplot(data=importance_df, x=\"Importance\", y=\"Feature\")\n",
        "plt.title(\"Feature Importance - Random Forest\")\n",
        "plt.tight_layout()\n",
        "plt.show()\n",
        "\n",
        "# User Prediction\n",
        "user_df = get_user_input()\n",
        "for col in [\"Department\", \"JobRole\"]:\n",
        "    user_df[col] = label_encoders[col].transform(user_df[col])\n",
        "\n",
        "user_scaled = scaler.transform(user_df)\n",
        "user_prediction = hybrid_model.predict(user_scaled)\n",
        "\n",
        "print(\"\\nPredicted Engagement for Input Employee:\", \"High\" if user_prediction[0] == 1 else \"Low\")\n",
        "\n",
        "if user_prediction[0] == 0:\n",
        "    print(\"\\nSuggestions to Improve Engagement:\")\n",
        "    if user_df[\"TrainingOpportunities\"].values[0] == 0:\n",
        "        print(\"- Provide more training opportunities.\")\n",
        "    if user_df[\"CareerProgression\"].values[0] == 0:\n",
        "        print(\"- Define clear career progression paths.\")\n",
        "    if user_df[\"LeadershipSupport\"].values[0] < 3:\n",
        "        print(\"- Increase leadership support through mentoring.\")\n",
        "    if user_df[\"WorkLifeBalance\"].values[0] < 5:\n",
        "        print(\"- Improve work-life balance with flexible hours.\")\n",
        "    if user_df[\"RemoteWorkAvailable\"].values[0] == 0:\n",
        "        print(\"- Consider offering remote work options.\")\n",
        "\n"
      ]
    }
